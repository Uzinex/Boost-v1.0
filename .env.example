# --- Server configuration ---
DATABASE_URL=postgresql://user:password@host:port/db
PORT=8080
NODE_ENV=production
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# --- Front-end (Vite) WebApp configuration ---
VITE_TELEGRAM_BOT_TOKEN=${TELEGRAM_BOT_TOKEN}
VITE_DATABASE_URL=${DATABASE_URL}
VITE_ALLOW_MANUAL_ONBOARDING=true
# Provide a JSON string with the fallback Telegram user profile for local testing.
VITE_DEFAULT_TELEGRAM_USER={"id":999999,"username":"demo_boost","first_name":"Boost","last_name":"Tester"}
